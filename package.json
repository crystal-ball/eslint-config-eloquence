{
  "name": "eslint-config-eloquence",
  "version": "13.7.0",
  "description": "Adaptive, robust, elqouent ESLint configuration",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/eslint-config-eloquence.git"
  },
  "keywords": [
    "eloquent",
    "eslint",
    "eslintconfig",
    "crystal-ball"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "NODE_ENV=test eslint --format=pretty *.js src",
    "test:unit": "jest *.spec.js",
    "format": "prettier --write '*.{js,json,md}' '**/*.{js,json,md,mdx,scss}' !CHANGELOG.md"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "babel-eslint": "10.1.0",
    "confusing-browser-globals": "1.0.9",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-formatter-pretty": "3.0.1",
    "eslint-import-resolver-node": "0.3.3",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0"
  },
  "devDependencies": {
    "@crystal-ball/commitizen-base": "2.4.0",
    "@crystal-ball/prettier-base": "2.1.0",
    "@crystal-ball/semantic-release-base": "2.16.0",
    "@types/jest": "25.2.1",
    "husky": "4.2.5",
    "jest": "25.4.0"
  },
  "config": {
    "commitizen": {
      "path": "@crystal-ball/commitizen-base"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "prettier": "@crystal-ball/prettier-base",
  "release": {
    "extends": [
      "@crystal-ball/semantic-release-base"
    ]
  }
}
