{
  "name": "eslint-config-eloquence",
  "version": "7.2.0",
  "description": "Crystal Ball Componentry org ESLint configuration",
  "main": "index.js",
  "scripts": {
    "test": "eslint *.js",
    "format": "prettier --write *.js"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/eslint-config-eloquence.git"
  },
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "dependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-customizable": "^5.2.0",
    "prettier": "^1.12.1",
    "semantic-release": "^15.1.8"
  },
  "config": {
    "commitizen": {
