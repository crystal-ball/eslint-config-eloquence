{
  "name": "eslint-config-eloquence",
  "version": "0.0.0-semantic",
  "description": "Adaptive, robust, elqouent ESLint configuration",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/eslint-config-eloquence.git"
  },
  "keywords": [
    "eloquent",
    "eslint",
    "eslintconfig",
    "prettier",
    "airbnb"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "scripts": {
    "commit": "git cz",
    "test": "NODE_ENV=test eslint *.js",
    "format": "prettier --write \"*.{js,md}\""
  },
  "dependencies": {
    "babel-eslint": "~10.0.1",
    "eslint": "~5.10.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "~3.4.0",
    "eslint-plugin-cypress": "^2.1.3",
    "eslint-plugin-flowtype": "~3.2.0",
    "eslint-plugin-import": "~2.14.0",
    "eslint-plugin-jsx-a11y": "~6.1.2",
    "eslint-plugin-prettier": "~3.0.0",
    "eslint-plugin-react": "~7.11.1"
  },
  "devDependencies": {
    "commitizen": "^3.0.5",
    "cz-customizable": "^5.3.0",
    "prettier": "^1.15.3",
    "semantic-release": "^15.12.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
