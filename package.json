{
  "name": "eslint-config-eloquence",
  "version": "11.0.0",
  "description": "Adaptive, robust, elqouent ESLint configuration",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/eslint-config-eloquence.git"
  },
  "keywords": [
    "eloquent",
    "eslint",
    "eslintconfig",
    "prettier",
    "airbnb"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "scripts": {
    "test": "NODE_ENV=test eslint *.js",
    "format": "prettier --write \"*.{js,md}\""
  },
  "dependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-cypress": "2.2.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0"
  },
  "devDependencies": {
    "@crystal-ball/commitizen-base": "1.0.0",
    "@crystal-ball/prettier-base": "1.0.0",
    "@crystal-ball/semantic-release-base": "1.0.1",
    "husky": "2.2.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-adapter-eslint"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  }
}
