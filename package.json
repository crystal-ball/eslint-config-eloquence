{
  "name": "eslint-config-eloquence",
  "version": "0.0.0-semantic",
  "description": "Adaptive, robust, elqouent ESLint configuration",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/eslint-config-eloquence.git"
  },
  "keywords": [
    "eloquent",
    "eslint",
    "eslintconfig",
    "prettier",
    "airbnb"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "scripts": {
    "commit": "git cz",
    "test": "NODE_ENV=test eslint *.js",
    "format": "prettier --write \"*.{js,md}\""
  },
  "dependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "~2.9.0",
    "eslint-plugin-flowtype": "~2.49.3",
    "eslint-plugin-import": "~2.13.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-prettier": "~2.6.0",
    "eslint-plugin-react": "~7.9.1"
  },
  "devDependencies": {
    "commitizen": "^2.10.1",
    "cz-customizable": "^5.2.0",
    "prettier": "^1.13.5",
    "semantic-release": "^15.6.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
