{
  "name": "eslint-config-eloquence",
  "version": "12.9.1",
  "description": "Adaptive, robust, elqouent ESLint configuration",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/eslint-config-eloquence.git"
  },
  "keywords": [
    "eloquent",
    "eslint",
    "eslintconfig",
    "prettier",
    "airbnb"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "scripts": {
    "test": "NODE_ENV=test eslint --format=pretty *.js src",
    "format": "prettier --write '*.{md,js,json}' '{.github,src}/**/*.{md,js,json}'"
  },
  "dependencies": {
    "babel-eslint": "10.0.3",
    "confusing-browser-globals": "1.0.9",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-formatter-pretty": "2.1.1",
    "eslint-plugin-cypress": "2.7.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0"
  },
  "devDependencies": {
    "@crystal-ball/commitizen-base": "2.1.0",
    "@crystal-ball/semantic-release-base": "2.3.0",
    "@crystal-ball/prettier-base": "1.5.0",
    "husky": "3.0.9"
  },
  "config": {
    "commitizen": {
      "path": "@crystal-ball/commitizen-base"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "prettier": "@crystal-ball/prettier-base",
  "release": {
    "extends": [
      "@crystal-ball/semantic-release-base"
    ]
  }
}
